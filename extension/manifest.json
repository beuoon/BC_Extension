{
    "name": "Browser Controller",
    "description": "단축키로 유튜브 뮤직과 트위치를 조작하세요!",
    "icons": {
        "128": "icon.png"
    },
    "version": "3.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://music.youtube.com/*"],
            "js": ["ym_control.js"]
        },
        {
            "matches": ["https://www.twitch.tv/*"],
            "js": ["tw_control.js"]
        }
    ]
}